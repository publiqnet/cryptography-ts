{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "publishing-platform": {
      "root": "projects/publishing-platform/",
      "sourceRoot": "projects/publishing-platform/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/publishing-platform",
            "index": "projects/publishing-platform/src/index.html",
            "main": "projects/publishing-platform/src/main.ts",
            "polyfills": "projects/publishing-platform/src/polyfills.ts",
            "tsConfig": "projects/publishing-platform/tsconfig.app.json",
            "assets": [
              "projects/publishing-platform/src/favicon.ico",
              "projects/publishing-platform/src/assets"
            ],
            "styles": [
              "projects/publishing-platform/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/publishing-platform/src/environments/environment.ts",
                  "with": "projects/publishing-platform/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "publishing-platform:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "publishing-platform:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "publishing-platform:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/publishing-platform/src/test.ts",
            "polyfills": "projects/publishing-platform/src/polyfills.ts",
            "tsConfig": "projects/publishing-platform/tsconfig.spec.json",
            "karmaConfig": "projects/publishing-platform/karma.conf.js",
            "styles": [
              "projects/publishing-platform/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/publishing-platform/src/favicon.ico",
              "projects/publishing-platform/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/publishing-platform/tsconfig.app.json",
              "projects/publishing-platform/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
              "builder": "@angular-devkit/build-angular:server",
              "options": {
                  "outputPath": "dist/server",
                  "main": "projects/publishing-platform/src/main.server.ts",
                  "tsConfig": "projects/publishing-platform/tsconfig.server.json"
              },
              "configurations": {
                  "production": {
                      "fileReplacements": [
                          {
                              "replace": "projects/publishing-platform/src/environments/environment.ts",
                              "with": "projects/publishing-platform/src/environments/environment.prod.ts"
                          }
                      ]
                  }
              }
          }
      }
    },
    "shared-lib": {
      "root": "projects/shared-lib",
      "sourceRoot": "projects/shared-lib/src",
      "projectType": "library",
      "prefix": "publiq",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/shared-lib/tsconfig.lib.json",
            "project": "projects/shared-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shared-lib/src/test.ts",
            "tsConfig": "projects/shared-lib/tsconfig.spec.json",
            "karmaConfig": "projects/shared-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/shared-lib/tsconfig.lib.json",
              "projects/shared-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "publishing-platform"
}