{
  "name": "publiq-angular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:fix": "ng lint --fix",
    "clear:dist": "rm -rf ./dist",
    "build:libs": "npm run clear:dist && git submodule update --recursive --remote && cd ./shared-angular-workspace && ng build helper-lib && cd ../ && ng build shared-lib ",
    "precommit": "npm run lint:fix && git add .",
    "watch:shared-lib": "ng build shared-lib --watch",
    "build:shared-lib": "ng build shared-lib",
    "pp:serve": "npm run build:libs && ng serve",
    "pp:build": "npm run pp:build:stage",
    "pp:start": "npm run pp:start:stage",
    "pp:build:stage": "ng build --configuration=staging && npm run pp:build:stage:ssr",
    "pp:build:stage:ssr": "ng run publishing-platform-app:server && npm run pp:webpack:server",
    "pp:start:stage": "npm run build:libs && npm run pp:build:stage && node dist/server.js",
    "pp:build:prod": "ng build --configuration=production && npm run pp:build:prod:ssr",
    "pp:build:prod:ssr": "ng run publishing-platform-app:server --configuration=production && npm run pp:webpack:server",
    "pp:start:prod": "npm run pp:build:prod && node dist/server.js",
    "pp:webpack:server": "webpack --config webpack.server.config.js --progress --colors --project_name=publishing-platform-app",
    "w:serve": "npm run build:libs && ng serve wallet-app --port=4201",
    "w:start:stage": "npm run build:libs && npm run w:build:stage && node dist/server.js",
    "w:build:stage": "ng build wallet-app --configuration=staging && npm run w:build:stage:ssr",
    "w:build:stage:ssr": "ng run wallet-app:server && npm run w:webpack:server",
    "w:webpack:server": "webpack --config webpack.server.config.js --progress --colors --project_name=wallet-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/material-moment-adapter": "^7.3.3",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "^7.2.7",
    "@angular/router": "~7.2.0",
    "@angular/upgrade": "^7.1.2",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/jquery": "2.0.48",
    "angular-froala-wysiwyg": "^2.7.6",
    "angular2-useful-swiper": "5.0.1",
    "angulartics2": "^5.1.2",
    "axios": "^0.18.0",
    "blockchain-models-ts": "git+ssh://github.com/publiqnet/blockchain-models-ts.git",
    "body-parser": "^1.18.3",
    "buffer": "^5.2.1",
    "cheerio": "^1.0.0-rc.2",
    "core-js": "^2.5.4",
    "countdown": "^2.6.0",
    "crypto-js": "^3.1.9-1",
    "cryptography-ts": "git+ssh://github.com/publiqnet/cryptography-ts.git",
    "express": "4.16.2",
    "froala-editor": "^2.9.1",
    "hammerjs": "^2.0.8",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "ngx-clipboard": "^12.0.0",
    "ngx-img-cropper": "^0.11.0",
    "ngx-infinite-scroll": "^7.0.1",
    "ngx-lazy-load-images": "^1.3.1",
    "ngx-perfect-scrollbar": "5.1.0",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "request": "^2.85.0",
    "rxjs": "6.3.3",
    "rxjs-compat": "^6.4.0",
    "stream": "0.0.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "helper-lib": "file:shared-angular-workspace/dist/helper-lib"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.43",
    "codelyzer": "~4.5.0",
    "cpy-cli": "1.0.1",
    "express": "4.16.2",
    "http-server": "0.10.0",
    "husky": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "ng-packagr": "^4.7.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.4.0",
    "reflect-metadata": "0.1.10",
    "ts-loader": "^4.5.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.1"
  }
}
